<h1>Lista użytkowników (<%= users.length %>)</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>Rola</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(u => { %>
      <tr>
        <td><%= u.id %></td>
        <td><%= u.email %></td>
        <td><%= u.role %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
