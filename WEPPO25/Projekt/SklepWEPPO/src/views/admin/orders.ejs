<h1>Lista zamówień (<%= orders.length %>)</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Użytkownik</th>
      <th>Status</th>
      <th>Zawartość</th>
    </tr>
  </thead>
  <tbody>
    <% orders.forEach(o => { %>
      <tr>
        <td><%= o.id %></td>
        <td><%= o.email %></td>
        <td><%= o.status %></td>
        <td>
          <a href="/order/<%= o.id %>">Zobacz</a>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>
